{
  "manifest_version": 2,

  "name": "WebAPI Tools",
  "description": "В помощь разработчикам 2GIS",
  "version": "1.0",
  
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["build/main.js"],
      "css": ["src/tip.css"],
      "all_frames": true
    }
  ],
  
  "permissions": [
    "activeTab",
    "<all_urls>"
  ]
}